    <py_script>import json
import requests
import pandas as pd
import numpy as np

country_names = unique_rows.values
region = list()
for name in country_names:
	response = requests.get("https://restcountries.com/v3.1/name/" + name[0] + "?fullText=true")
	data = json.loads(response.content)
	region.append(data[0]['region'])

unique_rows = pd.DataFrame(unique_rows)
unique_rows['country'] = country_names
unique_rows['region'] = region
</py_script>
