    <py_script># python script
import pandas as pd
import pymysql
from sqlalchemy import create_engine

# Database connection
user = 'root'
passw = 'root'
host =  'localhost' 
port = 3306 
database = 'pentaho'

mydb = create_engine('mysql+pymysql://' + user + ':' + passw + '@' + host + ':' + str(port) + '/' + database , echo=False)

dim_country_data = pd.read_sql('select * from dim_country', con=mydb)
dim_product_data = pd.read_sql('select * from dim_product', con=mydb) 
dim_retailer_data = pd.read_sql('select * from dim_retailer', con=mydb) 
dim_date_data = pd.read_sql("select date_tk, concat('Q',quarter,' ',year) 'qu' from dim_date", con=mydb) 

country_tk = df.merge(dim_country_data, left_on='Retailer country', right_on='country')['country_tk']
product_tk = df.merge(dim_product_data, left_on='Product', right_on='product_name')['product_tk']
retailer_tk = df.merge(dim_retailer_data, left_on='Retailer type', right_on='name')['retailer_tk']
date_tk = df.merge(dim_date_data, left_on='Quarter', right_on='qu')['date_tk']

df['Retailer country'] = country_tk
df['Product'] = product_tk
df['Retailer type'] = retailer_tk
df['Quarter'] = date_tk</py_script>
